	noteadd "軽減    : " + (100 - 10000 / (prot + 100)) + "% + " + protdice1 + "d" + protdice2
	noteadd ""
	noteadd "------------------------------ 装備品 合計重量" + cnvweight(cdata(CDATA_EQUIP_WEIGHT, cc)) + " " + cnveqweight(cc)
	noteadd ""
	procid = (procid + 1) \ 4
	proclist(procid) = "Dump:5" + " ct:" + ct
	listmax = 0
	repeat 30, 100
		if ( cdata(cnt, cc) != 0 ) {
			if ( trait(TRAIT_ETHER_EQUIP_NECK) != 0 ) {
