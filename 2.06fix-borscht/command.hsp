					continue
				}
			}
			if ( trait(TRAIT_ETHER_EQUIP_LEG) != 0 ) {
				if ( cdata(cnt, cc) / EXT_EQUIP_SLOTS == 9 ) {
					continue
				}
			}
			if ( trait(TRAIT_ETHER_EQUIP_BACK) != 0 ) {
				if ( cdata(cnt, cc) / EXT_EQUIP_SLOTS == 3 ) {
					continue
				}
			}
			if ( trait(TRAIT_ETHER_EQUIP_HEAD) != 0 ) {
				if ( cdata(cnt, cc) / EXT_EQUIP_SLOTS == 1 ) {
					continue
				}
			}
			if ( trait(TRAIT_ETHER_BONES) != 0 ) {
				if ( cdata(cnt, cc) / EXT_EQUIP_SLOTS == 4 ) {
					continue
				}
			}
			if ( trait(TRAIT_ETHER_BLADE_HANDS) != 0 ) {
				if ( cdata(cnt, cc) / EXT_EQUIP_SLOTS == 5 ) {
					continue
				}
			}
			if ( trait(TRAIT_ETHER_WEBBED) != 0 ) {
				if ( cdata(cnt, cc) / EXT_EQUIP_SLOTS == 6 ) {
					continue
				}
			}
			if ( trait(raitetherarm) != 0 ) {
				if ( cdata(cnt, cc) / EXT_EQUIP_SLOTS == 7 ) {
					continue
				}
			}
			if ( trait(TRAIT_ETHER_TAIL) != 0 ) {
				if ( cdata(cnt, cc) / EXT_EQUIP_SLOTS == 8 ) {
					continue
				}
			}
