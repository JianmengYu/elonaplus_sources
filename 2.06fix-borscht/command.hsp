	s(5) = "名声度    : " + cdata(CDATA_FAME, CHARA_PLAYER)
	s(6) = "カルマ    : " + cdata(CDATA_KARMA, CHARA_PLAYER)
	s(7) = "DV        : " + cdata(CDATA_DV, CHARA_PLAYER)
	s(8) = "PV        : " + cdata(CDATA_PV, CHARA_PLAYER)
	s(9) = ""
	procid = (procid + 1) \ 4
