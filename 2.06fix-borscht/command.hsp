			}
			s = bodyn(cdata(cnt, cc) / EXT_EQUIP_SLOTS)
			if ( strmid(s, 0, 2) == "　" ) {
				s = strmid(s, 2, 2)
			}
			noteadd s + ":"
			noteadd qqq + " " + qqq(1)
			repeat listmax
				noteadd listn(0, cnt)
			loop
			noteadd ""
		}
	loop
	procid = (procid + 1) \ 4
	proclist(procid) = "Dump:6" + " ct:" + ct
	noteadd "------------------------------ 特徴"
