		}
		if ( cdatan(CDATAN_NEWSEX, cnt) != "" ) {
			noteadd cdatan(CDATAN_NAME, cnt) + " " + racename + "の" + classname + " " + cdatan(CDATAN_NEWSEX, cnt) + " " + calcage(cnt) + "歳" + "  " + cdata(CDATA_HEIGHT, cnt) + "cm" + " " + cdata(CDATA_WEIGHT, cnt) + "kg"
		}
		s = "レベル " + cdata(CDATA_LEVEL, cnt) + " "
		if ( cbit(CHARA_BIT_MARRIED, cnt) == TRUE ) {
			s += "婚約済み "
		}
		noteadd s
		noteadd ""
