	redraw 0
	cs_bk = -1
	pagemax = (listmax - 1) / pagesize
	if ( page < 0 ) {
		page = pagemax
	}
	else {
		if ( page > pagemax ) {
			page = 0
		}
	}
	txtnew
	txt lang("どの設備を建設する？", "What do you want to build?")

*com_select_building_loop_WHILE1
	if ( 1 == 0 ) {
		goto *label_6327
	}
	redraw 0
	s = lang("設備一覧", "Building List"), strhint2 + strhint3
	display_window (windoww - 500) / 2 + inf_screenx, winposy(400), 500, 400
	keyrange = 0
	repeat pagesize
		p = pagesize * page + cnt
		if ( p >= listmax ) {
			break
		}
		key_list(cnt) = key_select(cnt)
		keyrange++
		y = wy + 60 + cnt * 70
		pos wx + 100, y
		gfini 350, 18
		gfdec2 12, 14, 16
		display_key wx + 88, y, cnt
	loop
	font lang(cfg_font1, cfg_font2), 14 - en * 2, 0
	cs_listbk
	repeat pagesize
		p = pagesize * page + cnt
		if ( p >= listmax ) {
			break
		}
		p = list(0, p)
		y = wy + 60 + cnt * 70
		pos wx + 20, y
		gmode 2
		gcopy BUFFER_MAP, bdref(0, p) * inf_tiles, 528, inf_tiles, inf_tiles
		cs_list bdrefn(p), wx + 114, y, 19
		color 0, 0, 0
		pos wx + 360, y + 2
		mes "" + bdref(1, p) + "k gp"
	loop
	if ( keyrange != 0 ) {
		cs_bk = cs
	}
	gosub *draw_budget
	redraw 1
	await cfg_wait1
	key_check
	cursor_check
	p = -1
	repeat keyrange
		if ( key == key_select(cnt) ) {
			p = list(0, pagesize * page + cnt)
			break
		}
	loop
	if ( p != (-1) ) {
		if ( mdata(MDATA_CITY_BUDGET) < bdref(1, p) ) {
			snd SOUNDLIST_FAIL1
			txtnew
			txt lang("予算が足りない…", "Your city can't afford it...")
			goto *com_select_building_loop_WHILE1_CONTINUE
		}
		mdata(MDATA_CITY_BUDGET) -= bdref(1, p)
		snd SOUNDLIST_BUILD1
		txtnew
		txt lang(bdrefn(p) + "を建設した！", "You have built a " + bdrefn(p) + "!")
		bddata(0, city, cbd) = p, tlocx, tlocy, bdref(0, p) + 363
		gosub *world_refresh
		return p
	}
	if ( key == key_pageup ) {
		if ( pagemax != 0 ) {
			snd SOUNDLIST_POP1
			page++
			goto *com_select_building_loop
		}
	}
	if ( key == key_pagedown ) {
		if ( pagemax != 0 ) {
			snd SOUNDLIST_POP1
			page--
			goto *com_select_building_loop
		}
	}
	if ( key == key_cancel ) {
		return -1
	}

