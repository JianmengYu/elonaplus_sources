#defcfunc prodcheck
	locvar_prodcheck_f = 1
	if ( matval(1) > sdata(matval, CHARA_PLAYER) ) {
		locvar_prodcheck_f = -1
	}
	repeat 6
		locvar_prodcheck_j = matneed(cnt * 2), matneed(cnt * 2 + 1)
		if ( locvar_prodcheck_j == (-1) ) {
			break
		}
		if ( mat(locvar_prodcheck_j) < locvar_prodcheck_j(1) ) {
			locvar_prodcheck_f = -1
		}
	loop
	return locvar_prodcheck_f

