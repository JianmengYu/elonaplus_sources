	if ( cdata(CDATA_FOV, CHARA_PLAYER) < 1 ) {
		cdata(CDATA_FOV, CHARA_PLAYER) = 1
	}
	if ( cdata(CDATA_FOV, CHARA_PLAYER) >= 17 ) {
		cdata(CDATA_FOV, CHARA_PLAYER) = 17 - 1
	}
	repeat 17
		y = cnt
		repeat 17
			x = cnt
			if ( dist(x, y, 17 / 2, 17 / 2) < (17 - 2) / 2 + 1 ) {
				fovmap(x, y) = 1
			}
		loop
	loop
	repeat 17
		y = cnt
		p(1) = 0
		repeat 17
			x = cnt
			p = fovmap(x, y)
			if ( p != 0 & p(1) == 0 ) {
				fovlist(0, y) = x
				p(1) = 1
			}
			if ( p == 0 & p(1) == 1 ) {
				fovlist(1, y) = x
				break
			}
		loop
	loop
