			break
		}
		if ( gdata(GDATA_AREA) == AREA_RUST_PLAZA ) {
			p = instr(buff, 0, "%CAVE," + lang("JP", "EN"))
			break
		}
		if ( cdata(CDATA_ROLE, tc) >= ROLE_SHOP_MIN & cdata(CDATA_ROLE, tc) < ROLE_GUEST_MIN | cdata(CDATA_ROLE, tc) == ROLE_GUEST_MERCHANT ) {
