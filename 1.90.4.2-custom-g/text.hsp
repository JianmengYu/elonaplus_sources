			break
		}
		if ( gdata(GDATA_AREA) == AREA_ULM_LESON ) {
			p = instr(buff, 0, "%SUMMON," + lang("JP", "EN"))
			break
		}
		if ( gdata(GDATA_AREA) == AREA_OL_DRAN ) {
